		{
			"name": "UpdateCourseDescription",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"const currentCase = pm.collectionVariables.get(\"currentCase\");",
							"switch(currentCase){",
							"    case \"noToken\":",
							"        console.log(\"Generating a request without JWT token in the header...\");",
							"        pm.collectionVariables.set(\"jwtToken\", null);",
							"        pm.collectionVariables.set(\"description\", pm.collectionVariables.get(\"firstDescription\"))",
							"        pm.collectionVariables.set(\"requestBody\", JSON.stringify({",
							"            description: pm.collectionVariables.get(\"description\")",
							"        }));",
							"        break;",
							"    case \"incorrectCourseIdFormat\":",
							"        console.log(\"Generating a request with incorrect course id format...\");",
							"        pm.collectionVariables.set(\"jwtToken\", pm.collectionVariables.get(\"firstJwtToken\"));",
							"        pm.collectionVariables.set(\"courseId\", pm.variables.replaceIn(\"{{$randomWord}}\"));",
							"        break;",
							"    case \"incorrectCourseInstructorCombination\":",
							"        console.log(\"Generating a request with incorrect instructor and course combination...\");",
							"        pm.collectionVariables.set(\"email\", pm.variables.replaceIn(\"{{$randomEmail}}\"));",
							"        pm.collectionVariables.set(\"password\", pm.variables.replaceIn(\"{{$randomPassword}}\"));",
							"        pm.sendRequest({",
							"            url: pm.collectionVariables.get(\"instructorRegistrationUrl\"),",
							"            method: 'POST',",
							"            header: {",
							"                'content-type': 'application/json'",
							"            },",
							"            body: {",
							"                mode: 'raw',",
							"                raw: JSON.stringify({",
							"                    email: pm.collectionVariables.get(\"email\"),",
							"                    password: pm.collectionVariables.get(\"password\")",
							"                })",
							"            }",
							"        }, function (err, res) {",
							"            console.log(\"Logging in as an instructor...\");",
							"            pm.collectionVariables.set(\"secondEmail\", pm.collectionVariables.get(\"email\"));",
							"            pm.collectionVariables.set(\"secondPassword\", pm.collectionVariables.get(\"password\"));",
							"            pm.sendRequest({",
							"                url: pm.collectionVariables.get(\"instructorLoginUrl\"),",
							"                method: 'POST',",
							"                header: {",
							"                    'content-type': 'application/json'",
							"                },",
							"                body: {",
							"                    mode: 'raw',",
							"                    raw: JSON.stringify({",
							"                        email: pm.collectionVariables.get(\"secondEmail\"),",
							"                        password: pm.collectionVariables.get(\"secondPassword\")",
							"                    })",
							"                }",
							"            }, function (err, res) {",
							"                console.log(\"Creating a course...\");",
							"                pm.collectionVariables.set(\"secondJwtToken\", res.json()[\"token\"]);",
							"                pm.collectionVariables.set(\"secondTitle\", pm.variables.replaceIn(\"{{$randomWords}}\"));",
							"                pm.collectionVariables.set(\"title\", pm.collectionVariables.get(\"secondTitle\"));",
							"                pm.sendRequest({",
							"                    url: pm.collectionVariables.get(\"courseCreationUrl\"),",
							"                    method: 'POST',",
							"                    header: {",
							"                        'content-type': 'application/json',",
							"                        'Authorization': 'Bearer ' + pm.collectionVariables.get('secondJwtToken')",
							"                    },",
							"                    body: {",
							"                        mode: 'raw',",
							"                        raw: JSON.stringify({",
							"                            title: pm.collectionVariables.get(\"title\")",
							"                        })",
							"                    }",
							"                }, function (err, res) {",
							"                    pm.collectionVariables.set(\"secondCourseId\", res.json()[\"ID\"]);",
							"                    pm.collectionVariables.set(\"courseId\", pm.collectionVariables.get(\"secondCourseId\"));",
							"                    pm.collectionVariables.set(\"jwtToken\", pm.collectionVariables.get(\"firstJwtToken\"));",
							"                });",
							"            });",
							"        });        ",
							"        break;",
							"    case \"incorrectCourseDescriptionFormat\":",
							"        console.log(\"Generating a request with incorrect instructor and course combination...\");",
							"        pm.collectionVariables.set(\"jwtToken\", pm.collectionVariables.get(\"firstJwtToken\"));",
							"        pm.collectionVariables.set(\"courseId\", pm.collectionVariables.get(\"firstCourseId\"));",
							"        pm.collectionVariables.set(\"description\", \"\");",
							"        pm.collectionVariables.set(\"requestBody\", JSON.stringify({",
							"            description: pm.collectionVariables.get(\"description\")",
							"        }));",
							"        break;",
							"    case \"correctRequest\":",
							"        console.log(\"Generating a correct request to update course description\");",
							"        pm.collectionVariables.set(\"description\", pm.variables.replaceIn(\"{{$randomLoremParagraph}}\"));",
							"        pm.collectionVariables.set(\"requestBody\", JSON.stringify({",
							"            description: pm.collectionVariables.get(\"description\")",
							"        }));",
							"        break;",
							"}",
							""
						],
						"type": "text/javascript"
					}
				},
